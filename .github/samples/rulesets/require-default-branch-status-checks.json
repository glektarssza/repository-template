{
    "name": "Require Status Checks to Pass before Merge to Default Branch",
    "target": "branch",
    "enforcement": "active",
    "conditions": {
        "ref_name": {
            "exclude": [],
            "include": [
                "~DEFAULT_BRANCH"
            ]
        }
    },
    "rules": [
        {
            "type": "required_status_checks",
            "parameters": {
                "strict_required_status_checks_policy": true,
                "do_not_enforce_on_create": false,
                "required_status_checks": [
                    {
                        "context": "Lint / Lint",
                        "integration_id": 15368
                    },
                    {
                        "context": "Test / Test",
                        "integration_id": 15368
                    },
                    {
                        "context": "Build / Build",
                        "integration_id": 15368
                    }
                ]
            }
        }
    ],
    "bypass_actors": [
        {
            "actor_id": 5,
            "actor_type": "RepositoryRole",
            "bypass_mode": "pull_request"
        }
    ]
}
